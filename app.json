
{
  "expo": {
    "name": "My Wishlist",
    "slug": "My Wishlist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/7ef63ee6-ea78-4ae0-8c0f-5dabcd379909.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/7ef63ee6-ea78-4ae0-8c0f-5dabcd379909.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MyWishlist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:mywishlist.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/7ef63ee6-ea78-4ae0-8c0f-5dabcd379909.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.MyWishlist",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*"
            },
            {
              "scheme": "http",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "text/plain"
            }
          ],
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/7ef63ee6-ea78-4ae0-8c0f-5dabcd379909.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/7ef63ee6-ea78-4ae0-8c0f-5dabcd379909.png",
          "color": "#000000",
          "defaultChannel": "price-drops"
        }
      ]
    ],
    "scheme": "mywishlist",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://dp5sm9gseg2u24kanaj9us8ayp8awmu3.app.specular.dev",
      "supabaseUrl": "https://YOUR_PROJECT_REF.supabase.co",
      "supabaseAnonKey": "YOUR_SUPABASE_ANON_KEY"
    }
  },
  "scheme": "mywishlist"
}
